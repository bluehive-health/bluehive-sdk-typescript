{
  "dxt_version": "0.2",
  "name": "@bluehive/sdk-mcp",
  "version": "0.1.0-alpha.20",
  "description": "The official MCP Server for the BlueHive API",
  "author": {
    "name": "BlueHive",
    "email": "wreiske@bluehive.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluehive-health/bluehive-sdk-typescript.git"
  },
  "homepage": "https://github.com/bluehive-health/bluehive-sdk-typescript/tree/main/packages/mcp-server#readme",
  "documentation": "https://docs.bluehive.com/",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "BLUEHIVE_API_KEY": "${user_config.BLUEHIVE_API_KEY}"
      }
    }
  },
  "user_config": {
    "BLUEHIVE_API_KEY": {
      "title": "api_key",
      "description": "API key for accessing BlueHive API endpoints. Format: \"ApiKey <your-api-key>\" or \"Bearer <your-api-key>\"",
      "required": true,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
